<mvc:View
	controllerName="sap.ui.walkthrough.controller.App"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true">
	<App>
		<pages>
			<Page title="{i18n>homePageTitle}">
				<content>
					<Panel
						headerText="{i18n>helloPanelTitle}"
						class="sapUiResponsiveMargin"
						width="auto">
						<content>
							<mvc:XMLView viewName="sap.ui.walkthrough.view.HelloPanel" />
						</content>
					</Panel>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>