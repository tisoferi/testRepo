<mvc:View
	controllerName="sap.ui.walkthrough.controller.Detail"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:walkthrough="sap.ui.walkthrough.control">
	<Page title="{i18n>detailPageTitle}"
		showNavButton="true"
		navButtonPress="onNavBack">
		<ObjectHeader
			intro="{invoice>ShipperName}"
			title="{invoice>Quantity} x {invoice>ProductName}"/>
		<walkthrough:ProductRating class="sapUiSmallMarginBeginEnd"
			change="onRatingChange" />
	</Page>
</mvc:View>